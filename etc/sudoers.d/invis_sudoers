Cmnd_Alias CREATEHOME  = /usr/sbin/createhome
wwwrun ALL=NOPASSWD:CREATEHOME
Cmnd_Alias DELETEHOME = /usr/sbin/deletehome
wwwrun ALL=NOPASSWD:DELETEHOME
Cmnd_Alias CREATEGROUPSHARE = /usr/sbin/creategroupshare
wwwrun ALL=NOPASSWD:CREATEGROUPSHARE
Cmnd_Alias FETCHPHP = /var/lib/cornaz/bin/fetchcopy
wwwrun ALL=NOPASSWD:FETCHPHP
Cmnd_Alias HOLIDAY = /var/lib/cornaz/bin/holiday
wwwrun ALL=NOPASSWD:HOLIDAY
Cmnd_Alias BACKHOME = /var/lib/cornaz/bin/backhome
wwwrun ALL=NOPASSWD:BACKHOME
